<p id="notice"><%= notice %></p>

<%= image_tag(@student.photo_url) %>

<p>
  <strong>Given name:</strong>
  <%= @student.given_name %>
</p>

<p>
  <strong>Family name:</strong>
  <%= @student.family_name %>
</p>

<p>
  <strong>Email:</strong>
  <%= @student.email %>
</p>

<p>
  <strong>Team:</strong>
  <%#= @student.team.name %>
  <%= link_to @student.team.name, team_path(@student.team_id) %>
</p>

<%= link_to 'Edit', edit_student_path(@student) %> |
<%= link_to 'Back', students_path %>
